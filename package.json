{
    "name": "asteriods",
    "displayName": "asteriods",
    "description": "asteriod game vscode ",
    "version": "0.0.2",
    "publisher": "arashmidos",
    "icon": "utils/vscode-asteroid.png",
    "engines": {
        "vscode": "^1.37.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.asteroid",
        "onCommand:extension.asteroid_web"
    ],
    "repository":{
        "type": "git",
        "url":"https://github.com/Arshiamidos/Asteroids_vscode_extension"
    },
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.asteroid",
                "title": "asteroid code 👩🏻‍💻 🚀 👨🏻‍💻"
            },
            {
                "command": "extension.asteroid_web",
                "title": "asteroid web  🌎 🚀 🌏"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/vscode": "^1.37.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "*"
    },
    "dependencies": {
        "@types/node-fetch": "^2.5.0",
        "fetch": "^1.1.0",
        "node-fetch": "^2.6.0",
        "vscode-test": "^1.2.0"
    }
}
